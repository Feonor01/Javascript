<script>

var list = [11, 3, 5, 1, 3, 6, 9];
var found = 0;
var text = "";
for(var i=0; i < list.length; i++){
	
	if(list[i] === 3 ){
	
	found++;
	text += i + " pozicija ";
	//document.write('i=' + i + '<br>');
	}
	//document.write('i=' + i + '<br>');

}

//console.log(i);
alert(text);
document.write('ima ih: ' + found);
</script>
